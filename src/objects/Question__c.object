    <validationRules>
        <fullName>CheckFinalIntoRepeated</fullName>
        <active>true</active>
        <description>Final questions cannot be added, or moved into a repeated section</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Type__c, &apos;end_of_survey&apos;) ,
    ISPICKVAL(Parent__r.Type__c, &apos;repeat&apos;)
)</errorConditionFormula>
        <errorMessage>Cannot add or move an end of survey question into a repeated section</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckMinimumNotGreaterThanMaximum</fullName>
        <active>true</active>
        <description>Cannot create a question with Minimum greater than Maximum</description>
        <errorConditionFormula>Minimum__c &gt;  Maximum__c</errorConditionFormula>
        <errorMessage>Cannot create a question with Minimum greater than Maximum</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckParent</fullName>
        <active>true</active>
        <description>Make sure that, unless the question is of type section or repeat, then the question has a parent.</description>
        <errorConditionFormula>AND(
    AND(
        NOT(ISPICKVAL(Type__c, &apos;section&apos;)),
        NOT(ISPICKVAL(Type__c, &apos;repeat&apos;))
    ),
    OR(
        ISNULL(Parent__c),
        ISBLANK(Parent__c)
    )
)</errorConditionFormula>
        <errorMessage>Only questions of type SECTION and REPEAT can have no parent. If question is not a section or repeat then its parent must be a section or repeat</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckRepeatedSourceFields</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
    AND(
     NOT(ISPICKVAL(Type__c,&apos;repeat&apos;)),
     OR(
          NOT(ISNULL(RepeatTimes__c)),
          LEN(RepeatSourceValue__c)&gt;0
       )
        ),
     AND(
     ISPICKVAL(Type__c,&apos;repeat&apos;),
     OR(
        AND(
            NOT(ISNULL(RepeatTimes__c)),
            LEN(RepeatSourceValue__c)&gt;0
         ),AND(
             LEN(RepeatSourceValue__c)&gt;0,
             NOT(ISPICKVAL( RepeatSourceValue__r.Type__c ,&apos;number-integer&apos;))
           )
      )
    )
    )</errorConditionFormula>
        <errorMessage>Repeat Times and Repeat Source Value fields can only be set in Repeat questions,and only one can be set simultaneously</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FirstVersionLastVersion</fullName>
        <active>true</active>
        <errorConditionFormula>FromVersion__c  &gt; ToVersion__c</errorConditionFormula>
        <errorMessage>The first version value must be lesser or equal than last version</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckCascadingSelectInCascadingQuestions</fullName>
        <active>true</active>
        <description>If question type is cascading select, then this question must be related to a cascading library</description>
        <errorConditionFormula>
            AND(
                ISPICKVAL(Type__c, 'cascading-select') ,
                CascadingSelect__c = '' || CascadingSelect__c = null
            )
        </errorConditionFormula>
        <errorMessage>Cascading select is required for this type of question</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckCascadingOnRepeat</fullName>
        <active>true</active>
        <description>If question type is cascading select then parent cannot be repeat section</description>
        <errorConditionFormula>
            AND( 
                ISPICKVAL(Type__c, 'cascading-select') , 
                ISPICKVAL(Parent__r.Type__c, 'repeat')
            )
        </errorConditionFormula>
        <errorMessage>Cascading Selects are not allowed in a Repeat section.</errorMessage>
    </validationRules>

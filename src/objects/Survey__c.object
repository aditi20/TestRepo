    <validationRules>
        <fullName>AllowAnonymousInterviewee_Must_Be_False</fullName>
        <active>true</active>
        <description>AllowAnonymousInterviewee__c must be set to false</description>
        <errorConditionFormula>AllowAnonymousInterviewee__c</errorConditionFormula>
        <errorDisplayField>AllowAnonymousInterviewee__c</errorDisplayField>
        <errorMessage>AllowAnonymousInterviewee__c must be set to false</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_Code_Should_Have_A_Match</fullName>
        <active>true</active>
        <description>Validate that the country code have a match in the country list</description>
        <errorConditionFormula>IsPPI__c &amp;&amp;
VLOOKUP( $ObjectType.ISO_Country__c.Fields.Name,
    $ObjectType.ISO_Country__c.Fields.Name,
    CountryCode__c ) = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>CountryCode__c</errorDisplayField>
        <errorMessage>The selected country code is not valid. This should be an ISO 3166-1 alpha-2 country code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InterviewMapField_Must_Be_Null</fullName>
        <active>true</active>
        <description>InterviewMapField__c must be null as it is no longer used</description>
        <errorConditionFormula>NOT(ISBLANK(InterviewMapField__c ))</errorConditionFormula>
        <errorDisplayField>InterviewMapField__c</errorDisplayField>
        <errorMessage>Please leave the InterviewMapField__c blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UniqueName</fullName>
        <active>true</active>
        <description>Validate the name uniqueness.</description>
        <errorConditionFormula>Name = VLOOKUP( $ObjectType.Survey__c.Fields.Name,
                                 $ObjectType.Survey__c.Fields.Name,
                                 Name )
&amp;&amp; Id &lt;&gt; VLOOKUP( $ObjectType.Survey__c.Fields.Id,
                                  $ObjectType.Survey__c.Fields.Name,
                                  Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The name of the survey must be unique</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>YearFormat</fullName>
        <active>true</active>
        <description>Validates that the year should be an integer greater than 1900</description>
        <errorConditionFormula>PPIDataYear__c &lt; 1900</errorConditionFormula>
        <errorDisplayField>PPIDataYear__c</errorDisplayField>
        <errorMessage>The year should be greater than 1900</errorMessage>
    </validationRules>
